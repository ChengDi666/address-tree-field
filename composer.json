{
    "name": "norgeit/address-tree-field",
    "description": "Nova Tree address filter component.",
    "keywords": [
        "laravel",
        "nova",
        "belongs-to-many",
        "nested-sets"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "kalnoy/nestedset": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Norgeit\\AddressTreeField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Norgeit\\AddressTreeField\\AddressTreeField"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
